<van-notify id="van-notify" />

<van-grid column-num="3">
  <van-grid-item bind:click="sharp" icon="data:image/png;base64," text="增强与锐化" />
  <van-grid-item bind:click="enhance" icon="data:image/png;base64," text="增强" />
  <van-grid-item bind:click="black_white" icon="data:image/png;base64," text="黑白" />
</van-grid>
<view class="showimg">
<van-icon bind:click="before_img" name="arrow-left" size="30px"/>
<van-image width="300" height="424" src="{{glo_img}}" />
<van-icon bind:click="after_img" name="arrow" size="30px"/>
</view>

<van-goods-action fixed="false" safe-area-inset-bottom="true">
  <van-goods-action-icon icon="data:image/png;base64," text="旋转" bind:click="onClickIcon" />
  <van-goods-action-icon icon="data:image/png;base64," text="转文字" bind:click="onClickIcon" />
  <van-goods-action-icon icon="data:image/png;base64," text="标注" />
  <van-goods-action-button text="点击保存" bind:click="saveimg" type="primary"/>
</van-goods-action>